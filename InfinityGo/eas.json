{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }, 
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.1.8:8000/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}